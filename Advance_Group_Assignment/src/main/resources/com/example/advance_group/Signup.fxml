<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="672.0" prefWidth="600.0" styleClass="root" stylesheets="@loginsignup.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.advance_group.LoginSignup">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <AnchorPane layoutX="116.0" layoutY="26.0" prefHeight="661.0" prefWidth="398.0" styleClass="boxbg">
            <children>
                <Label alignment="CENTER" layoutX="115.0" layoutY="128.0" prefHeight="41.0" prefWidth="166.0" text="Sign Up" textAlignment="RIGHT">
                    <font>
                        <Font name="System Bold" size="27.0" />
                    </font></Label>
                <PasswordField fx:id="signuppassword" alignment="CENTER" layoutX="73.0" layoutY="403.0" prefHeight="30.0" prefWidth="275.0" promptText="Password" styleClass="inputfield" stylesheets="@loginsignup.css">
                    <font>
                        <Font size="22.0" />
                    </font>
                </PasswordField>
                <TextField fx:id="signupusername" alignment="CENTER" layoutX="74.0" layoutY="274.0" promptText="Name" styleClass="inputfield">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <font>
                        <Font size="22.0" />
                    </font>
                </TextField>
                <Label alignment="CENTER" layoutX="44.0" layoutY="573.0" prefHeight="41.0" prefWidth="200.0" text="Don't have an account?" textAlignment="RIGHT">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Button fx:id="gotologin" layoutX="221.0" layoutY="572.0" mnemonicParsing="false" onAction="#gologin" prefHeight="44.0" prefWidth="96.0" styleClass="pagechangebutton" text="Log In">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Button fx:id="signupbtn" layoutX="116.0" layoutY="469.0" mnemonicParsing="false" onAction="#Validatesignup" prefHeight="31.0" prefWidth="166.0" styleClass="loginsignup" text="Sign Up" />
                <Button fx:id="usercancel" layoutX="115.0" layoutY="519.0" mnemonicParsing="false" onAction="#getcancel" prefHeight="31.0" prefWidth="166.0" styleClass="loginsignup" text="CANCEL" />
                <TextField fx:id="signupemail" alignment="CENTER" layoutX="74.0" layoutY="342.0" promptText="Username" styleClass="inputfield">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <font>
                        <Font size="22.0" />
                    </font>
                </TextField>
                <Label fx:id="displaylsignupmessage" alignment="CENTER" layoutX="98.0" layoutY="218.0" prefHeight="41.0" prefWidth="200.0" textAlignment="RIGHT">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
            <Button fx:id="goadminsignup" layoutX="144.0" layoutY="14.0" mnemonicParsing="false" onAction="#goadminsignuppage" prefHeight="44.0" prefWidth="116.0" styleClass="pagechangebutton" text="Admin Signup">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
